{
  "name": "centagon/frizzle",
  "description": "Opinionated event bus over HTTP",
  "keywords": [
    "laravel",
    "event",
    "event-bus",
    "http"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jelle Spekken",
      "email": "jelle.spekken@centagon.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/database": "~3.5",
    "orchestra/testbench": "~3.5",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Centagon\\Frizzle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Centagon\\Frizzle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Centagon\\Frizzle\\FrizzleServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
